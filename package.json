{
  "name": "lambda-form",
  "version": "1.3.0",
  "description": "A form submission handler using Serverless and AWS Lambda",
  "main": "handler.js",
  "scripts": {
    "deploy": "sls deploy && sls s3deploy",
    "deploy-prod": "sls deploy --stage prod && sls s3deploy --stage prod",
    "lint": "eslint --ext .js,.jsx ./",
    "lint-fix": "eslint --fix --ext .js,.jsx ./",
    "logs": "sls logs -f formPostHandler -t",
    "serverless": "node_modules/.bin/serverless",
    "sls": "node_modules/.bin/serverless",
    "service-info": "sls info",
    "local": "node_modules/.bin/serverless offline --stage dev",
    "prod": "pm2 start pm2.json --no-daemon"
  },
  "author": "Tom Noogen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/niiknow/lambda-form.git"
  },
  "dependencies": {
    "busboy": "^0.2.14",
    "consolidate": "^0.15.1",
    "debug": "^4.1.1",
    "mjml": "^4.3.1",
    "nodemailer": "^4.7.0",
    "nunjucks": "^3.2.0",
    "recaptcha2": "^1.3.3",
    "source-map-support": "^0.5.10",
    "temp": "^0.8.3",
    "uuid": "^3.3.2",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.3.4",
    "aws-sdk": "^2.416.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-source-map-support": "^2.0.1",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.1.6",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^21.27.2",
    "jest": "^24.3.0",
    "serverless": "^1.38.0",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-offline": "^3.33.0",
    "serverless-plugin-existing-s3": "^2.3.3",
    "serverless-webpack": "^5.2.0",
    "webpack": "^4.29.6",
    "webpack-node-externals": "^1.7.2"
  }
}
